            <string>// animacion
s_ani_osci(id, 0, m_ani_monsters, 0.1);
if reloj_comer != 0 {
    reloj_comer -= dlt;
    if reloj_comer &lt;= 0 {
        reloj_comer = 0;
        var aux = instance_create(x, y + 1, o_fosil);
        aux.culpa = m_culpa_lujuria;
    }
}
// ejecutar accion especial
reloj_accion -= dlt;
if reloj_accion &lt;= 0 {
    reloj_accion += m_reloj_accion + random(1);
    if despierto {
        // crear flores de busqueda por division y conquista
        var ok = true;
        with o_flor {
            if division == 2 {
                ok = false;
                break;
            }
        }
        if ok {
            repeat 4 {
                instance_create(x + random_range(-8, 8),
                    y + random_range(-8, 8), o_flor);
            }
        }
    }
}
// colision con objetos, luego ejecutar IA
if s_colisiones(id) and despierto {
    if lover != noone {
        if instance_exists(lover) {
            if lover.culpa == m_culpa_lujuria {
                direction = point_direction(x, y, lover.x, lover.y);
                var v = m_velocidad * dlt;
                x += lengthdir_x(v, direction);
                y += lengthdir_y(v, direction);
                if place_meeting(x, y, lover) {
                    if reloj_comer != 0 {
                        var aux = instance_create(x, y + 1, o_fosil);
                        aux.culpa = m_culpa_lujuria;
                    }
                    reloj_comer = m_comer_reloj;
                    with lover {
                        instance_destroy();
                    }
                    lover = noone;
                }
            }
            else {
                lover = noone;
            }
        }
        else {
            lover = noone;
        }
    }
}
// prevenir salirse del mundo
s_limites(id);

</string>
