    var v = m_flor_velocidad * dlt * (1 + division) / ds_list_size(ddd);
    var d;
    for (var i = 0; i &lt; ds_list_size(ddd); i++) {
        d = ds_list_find_value(ddd, i);
        x += lengthdir_x(v, d);
        y += lengthdir_y(v, d);
    }
}
ds_list_destroy(ddd);
// evitar escape del mundo
if x &lt; rango or x &gt; room_width - rango or
        y &lt; rango or y &gt; room_height - rango {
    var v = m_flor_velocidad * dlt * (1 + division);
    var d = point_direction(x, y, room_width / 2, room_height / 2);
    x += lengthdir_x(v, d);
    y += lengthdir_y(v, d);
}
x = clamp(x, 64, room_width - 64);
y = clamp(y, 192, room_height - 32);
depth = -y;
// ejecutar sus funciones
if random(1) &lt; 0.2 {
    s_flor(division);
}

